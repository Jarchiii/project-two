{{!-- this is going to be a table with edit and delete options--}}

<div class="big-container manage-photographer-wrapper pages content">
  <h3 class="manage-photographers-title">Manage your Photographers catalog</h3>
  <table class="photographer-manage-table">
    <thead>
      <tr class="table-row">
        <th class="table-head">Photographer Profile Picture</th>
        <th class="table-head">Photographer Name</th>
        <th class="table-head">Description</th>
        <th class="table-head">Photo Specialties</th>
        <th class="table-head">Price</th>
        <th class="table-head" >Edit</th>
        <th class="table-head">Delete</th>
      </tr>
    </thead>

    <tbody>
      {{#each photographers}}
      <tr class="table-row">
        {{> dashboard_photographer_row}} 
      </tr>
      {{/each}}
      {{#unless photographers}}
      <tr>
        <td colspan="6">sorry no photographer yet</td>
      </tr>
      {{/unless}}
    </tbody>
  </table>
</div>