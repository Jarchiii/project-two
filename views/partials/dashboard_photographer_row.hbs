<td class="table-division">
  <div><img class="row-pic" src="{{this.profile_picture}}" alt="img" class="table-image" /></div>
</td>
<td class="table-division">
  <div>{{this.name}}</div>
</td>
<td class="table-division">
  <div class="description">{{this.description}}</div>
</td>
<td class="table-division">
  <div class="categories">
    {{#each this.categories}}
    <li>{{this}}</li>
    {{/each}}
  </div>
</td>
<td class="table-division">
  <div>${{this.price}}</div>
</td>
<td class="table-division">
  <a href="/photographers/{{this._id}}/edit" data-id-photographer="{{this._id}}" class="fa fa-edit table-icon">edit</a>
</td>
<td class="table-division">
  <a href="/photographers/{{this._id}}/delete"><span class="fa fa-trash table-icon" data-id-sneaker="{{this._id}}"  aria-hidden="true">Delete</span></a> 
</td>
